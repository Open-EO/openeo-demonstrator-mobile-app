{
  "name": "openeo_demonstrator_app",
  "version": "0.1.3",
  "description": "App demonstrating openEO functionality in a mobile environment using the openEO JavaScript client.",
  "author": "Solenix Schweiz GmbH",
  "homepage": "https://www.solenix.ch/",
  "scripts": {
    "ng": "ng",
    "ionic": "ionic",
    "cordova": "cordova",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "serve": "ionic serve",
    "build": "ionic build",
    "build-ios": "ionic cordova build ios -- --prod --release --buildFlag=\"-UseModernBuildSystem=0\"",
    "build-android": "ionic cordova build android -- --prod --release -- --packageType=bundle",
    "package-ios": "ionic cordova build ios -- --prod --release --device --codeSignIdentity=\"iPhone Developer\" --buildFlag=\"-UseModernBuildSystem=0\"",
    "ios-live": "ionic cordova emulate ios -lc --debug -- --buildFlag=\"-UseModernBuildSystem=0\"",
    "android-live": "ionic cordova emulate android -lc --debug --ssl",
    "prettier:run": "prettier --write \"src/{app,environments,assets,theme}/**/*{.ts,.js,.json,.css,.scss}\"",
    "prettier:check": "prettier -c \"src/{app,environments,assets,theme}/**/*{.ts,.js,.json,.css,.scss}\"",
    "prettier:quick": "pretty-quick --pattern \"src/{app,environments,assets,theme}/**/*{.ts,.js,.json,.css,.scss}\""
  },
  "private": true,
  "dependencies": {
    "@angular/common": "~8.2.14",
    "@angular/core": "~8.2.14",
    "@angular/forms": "~8.2.14",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/router": "~8.2.14",
    "@ionic-native/core": "^5.0.0",
    "@ionic-native/diagnostic": "^5.27.0",
    "@ionic-native/geolocation": "^5.0.0",
    "@ionic-native/keyboard": "^5.0.0",
    "@ionic-native/social-sharing": "^5.28.0",
    "@ionic-native/splash-screen": "^5.0.0",
    "@ionic-native/status-bar": "^5.0.0",
    "@ionic/angular": "^5.0.0",
    "@ionic/storage": "^2.2.0",
    "@ngxs/logger-plugin": "^3.6.2",
    "@ngxs/router-plugin": "^3.6.2",
    "@ngxs/store": "^3.6.2",
    "@openeo/js-client": "^1.0.1",
    "@types/crypto-js": "^3.1.47",
    "cesium": "^1.66.0",
    "cordova-res": "^0.15.1",
    "core-js": "^2.5.4",
    "crypto-js": "^4.0.0",
    "d3": "^5.15.0",
    "geotiff": "^1.0.0-beta.7",
    "hammerjs": "^2.0.8",
    "lodash": "^4.17.19",
    "native-run": "^0.3.0",
    "proj4": "^2.6.0",
    "rxjs": "~6.5.1",
    "simplify-js": "^1.2.4",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.900.5",
    "@angular-devkit/build-angular": "^0.803.25",
    "@angular-devkit/schematics": "^9.0.5",
    "@angular/cli": "~8.3.23",
    "@angular/compiler": "~8.2.14",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/language-service": "~8.2.14",
    "@ionic/angular-toolkit": "^2.3.0",
    "@ionic/cli": "^6.11.0",
    "@types/cesium": "^1.66.2",
    "@types/d3": "^5.7.2",
    "@types/hammerjs": "^2.0.36",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "@types/rx": "^4.1.1",
    "codelyzer": "^5.0.0",
    "cordova": "^10.0.0",
    "cordova-android": "^9.0.0",
    "cordova-ios": "^6.1.1",
    "cordova-plugin-androidx": "^2.0.0",
    "cordova-plugin-androidx-adapter": "^1.1.1",
    "cordova-plugin-device": "^2.0.2",
    "cordova-plugin-geolocation": "^4.0.2",
    "cordova-plugin-ionic-keyboard": "^2.2.0",
    "cordova-plugin-ionic-webview": "^5.0.0",
    "cordova-plugin-splashscreen": "^6.0.0",
    "cordova-plugin-statusbar": "^2.4.2",
    "cordova-plugin-whitelist": "^1.3.4",
    "cordova-plugin-x-socialsharing": "^6.0.1",
    "cordova-sqlite-storage": "4.0.0",
    "cordova.plugins.diagnostic": "5.0.1",
    "es6-promise-plugin": "^4.2.2",
    "fs": "0.0.1-security",
    "husky": "^1.3.1",
    "install": "^0.13.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "path": "^0.12.7",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "protractor": "~5.4.0",
    "q": "^1.5.1",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.4.3"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-device": {},
      "cordova-plugin-statusbar": {},
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-ionic-keyboard": {},
      "cordova-sqlite-storage": {},
      "cordova.plugins.diagnostic": {
        "ANDROID_SUPPORT_VERSION": "28.+"
      },
      "cordova-plugin-geolocation": {},
      "cordova-plugin-ionic-webview": {},
      "cordova-plugin-whitelist": {},
      "cordova-plugin-x-socialsharing": {
        "PHOTO_LIBRARY_ADD_USAGE_DESCRIPTION": "This app requires photo library access to function properly.",
        "PHOTO_LIBRARY_USAGE_DESCRIPTION": "This app requires photo library access to function properly."
      },
      "cordova-plugin-androidx": {},
      "cordova-plugin-androidx-adapter": {}
    },
    "platforms": [
      "android",
      "ios"
    ]
  },
  "browser": {
    "fs": false,
    "path": false,
    "os": false,
    "http": false,
    "https": false,
    "crypto": false,
    "stream": false
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier:quick -- --staged && npm run lint"
    }
  }
}
